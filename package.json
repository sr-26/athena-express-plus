{
  "name": "athena-express-plus",
  "version": "8.1.0",
  "description": "Athena-Express-Plus is a Node.js library for simplified SQL query execution in Amazon Athena, supporting the latest AWS SDK v3 architecture. It provides seamless integration with Athena, enabling clean JSON result retrieval, parameterized queries, and pagination",
  "main": "./lib/index.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sr-26/athena-express-plus"
  },
  "keywords": [
    "aws",
    "athena",
    "amazon",
    "amazon",
    "web",
    "services",
    "sql",
    "database",
    "amazon athena",
    "athena express",
    "sql queries",
    "aws sdk",
    "node.js library",
    "serverless",
    "data analysis",
    "parameterized queries",
    "pagination",
    "json results",
    "data warehousing",
    "aws integration",
    "serverless sql",
    "aws lambda",
    "query execution",
    "data querying",
    "aws services"
  ],
  "author": "Weasely",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sr-26/athena-express-plus"
  },
  "homepage": "https://github.com/sr-26/athena-express-plus#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "istanbul": "^0.4.5",
    "mocha": "^8.3.2",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "^3.540.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "csvtojson": "^2.0.10"
  }
}
